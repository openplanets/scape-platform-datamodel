    </xs:complexType>

    <xs:complexType name="tgroup">
        <xs:sequence>
            <xs:element name="thead" type="thead" minOccurs="0"/>
            <xs:element name="tbody" type="tbody"/>
        </xs:sequence>
        <xs:attribute name="cols" use="required" type="xs:nonNegativeInteger"/>
