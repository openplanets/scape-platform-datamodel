    <xs:simpleType name="av.level">
        <xs:restriction base="xs:token">
            <xs:enumeration value="fonds"/>
            <xs:enumeration value="subfonds"/>
            <xs:enumeration value="class"/>
            <xs:enumeration value="subclass"/>
            <xs:enumeration value="series"/>
            <xs:enumeration value="subseries"/>
            <xs:enumeration value="file"/>
            <xs:enumeration value="item"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="data.repositorycode">
        <xs:restriction base="xs:token">
            <xs:pattern
                value="((AF|AX|AL|DZ|AS|AD|AO|AI|AQ|AG|AR|AM|AW|AU|AT|AZ|BS|BH|BD|BB|BY|BE|BZ|BJ|BM|BT|BO|BA|BW|BV|BR|IO|BN|BG|BF|BI|KH|CM|CA|CV|KY|CF|TD|CL|CN|CX|CC|CO|KM|CG|CD|CK|CR|CI|HR|CU|CY|CZ|DK|DJ|DM|DO|EC|EG|SV|GQ|ER|EE|ET|FK|FO|FJ|FI|FR|GF|PF|TF|GA|GM|GE|DE|GH|GI|GR|GL|GD|GP|GU|GT|GN|GW|GY|HT|HM|VA|HN|HK|HU|IS|IN|ID|IR|IQ|IE|IL|IT|JM|JP|JO|KZ|KE|KI|KP|KR|KW|KG|LA|LV|LB|LS|LR|LY|LI|LT|LU|MO|MK|MG|MW|MY|MV|ML|MT|MH|MQ|MR|MU|YT|MX|FM|MD|MC|MN|MS|MA|MZ|MM|NA|NR|NP|NL|AN|NC|NZ|NI|NE|NG|NU|NF|MP|NO|OM|PK|PW|PS|PA|PG|PY|PE|PH|PN|PL|PT|PR|QA|RE|RO|RU|RW|SH|KN|LC|PM|VC|WS|SM|ST|SA|SN|CS|SC|SL|SG|SK|SI|SB|SO|ZA|GS|ES|LK|SD|SR|SJ|SZ|SE|CH|SY|TW|TJ|TZ|TH|TL|TG|TK|TO|TT|TN|TR|TM|TC|TV|UG|UA|AE|GB|US|UM|UY|UZ|VU|VE|VN|VG|VI|WF|EH|YE|ZM|ZW)|([a-zA-Z]{1})|([a-zA-Z]{3,4}))(-[a-zA-Z0-9:/\-]{1,11})"
            />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="notEmtpyToken">
        <xs:restriction base="xs:token">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="notEmtpyString">
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="ead-c">
        <xs:all>
            <xs:element name="did" type="did" minOccurs="1" maxOccurs="1"/>
            <xs:element name="scopecontent" type="scopecontent" minOccurs="1" maxOccurs="1"/>
            <xs:element name="accessrestrict" type="accessrestrict" minOccurs="0" maxOccurs="1"/>
            <xs:element name="accruals" type="accruals" minOccurs="0" maxOccurs="1"/>
            <xs:element name="acqinfo" type="acqinfo" minOccurs="0" maxOccurs="1"/>
            <xs:element name="appraisal" type="appraisal" minOccurs="0" maxOccurs="1"/>
            <xs:element name="arrangement" type="arrangement" minOccurs="0" maxOccurs="1"/>
            <xs:element name="bibliography" type="bibliography" minOccurs="0" maxOccurs="1"/>
            <xs:element name="bioghist" type="bioghist" minOccurs="0" maxOccurs="1"/>
            <xs:element name="custodhist" type="custodhist" minOccurs="0" maxOccurs="1"/>
            <xs:element name="otherfindaid" type="otherfindaid" minOccurs="0" maxOccurs="1"/>
            <xs:element name="phystech" type="phystech" minOccurs="0" maxOccurs="1"/>
            <xs:element name="prefercite" type="prefercite" minOccurs="0" maxOccurs="1"/>
            <xs:element name="relatedmaterial" type="relatedmaterial" minOccurs="0" maxOccurs="1"/>
            <xs:element name="userestrict" type="userestrict" minOccurs="0" maxOccurs="1"/>
            <xs:element name="note" type="note" minOccurs="0" maxOccurs="1"/>
        </xs:all>
        <xs:attribute name="level" type="av.level" use="required"/>
    </xs:complexType>

    <xs:complexType name="did">
        <xs:all>
            <xs:element name="unitid" type="unitid" minOccurs="1" maxOccurs="1"/>
            <xs:element name="unittitle" type="notEmtpyString" minOccurs="1" maxOccurs="1"/>
            <xs:element name="unitdate" type="unitdate" minOccurs="0" maxOccurs="1"/>
            <xs:element name="physdesc" type="physdesc" minOccurs="0" maxOccurs="1"/>
            <xs:element name="origination" type="notEmtpyString" minOccurs="1" maxOccurs="1"/>
            <xs:element name="langmaterial" type="langmaterial" minOccurs="0" maxOccurs="1"/>
            <xs:element name="materialspec" type="notEmtpyString" minOccurs="0" maxOccurs="1"/>
        </xs:all>
    </xs:complexType>

